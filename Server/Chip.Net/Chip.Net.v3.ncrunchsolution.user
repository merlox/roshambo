    <TestsWindowTreeLayout>
      <Column>
        <Name>Name</Name>
        <Width>588</Width>
      </Column>
      <Column>
        <Name>Status</Name>
        <Width>83</Width>
      </Column>
      <Column>
        <Name>LastExecutionTime</Name>
        <Width>81</Width>
      </Column>
      <Column>
        <Name>GridNode</Name>
        <Width>147</Width>
      </Column>
    </TestsWindowTreeLayout>
  </Settings>
</SolutionConfiguration>